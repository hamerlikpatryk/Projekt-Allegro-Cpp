		//////////////////////////////////// EVENTY PO KLIKNIĘCIU AUTORÓW
		if (autorzy_on == 1)
		{
			al_draw_bitmap(autorzy, 0, 0, NULL);
			al_draw_rectangle(x, y, x + 10, y + 10, playerColor, 1);
			if (x > 0 && x < 160 && y >0 && y < 90)
				if (event.type == ALLEGRO_EVENT_MOUSE_BUTTON_DOWN)
				{
					if (event.mouse.button & 1)
					{
						autorzy_on = 0;
