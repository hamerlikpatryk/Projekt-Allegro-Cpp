		al_flip_display();
	}

	al_destroy_display(display);
	al_uninstall_keyboard();
	al_uninstall_mouse();
	al_destroy_timer(timer);

	al_destroy_bitmap(menu);
	al_destroy_bitmap(nowagraakt);
	al_destroy_bitmap(wczytajpoziomakt);
