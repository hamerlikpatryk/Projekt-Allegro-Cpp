			al_draw_bitmap(stol1, okno_x+453, 240 + 63, NULL); //4
			al_draw_bitmap(stol1, okno_x+851, 240 + 63, NULL);
			al_draw_bitmap(stol1, okno_x+1524, 240 + 63, NULL);
			al_draw_bitmap(stol1, okno_x+1847, 240 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+420, 182 + 63, NULL);  //15 wrogow
			al_draw_bitmap(wrog1, okno_x+501, 201 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+729, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+781, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+910, 201 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1042, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1094, 182 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1185, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1326, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1448, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1489, 182 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1586, 201 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1790, 234 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+1899, 201 + 63, NULL);
			al_draw_bitmap(wrog1, okno_x+2087, 234 + 63, NULL); // 
			al_draw_bitmap(wrog2, okno_x+2163, 197 + 63, NULL);
			al_draw_bitmap(lutnia1, okno_x+2201, 237 + 63, NULL);
