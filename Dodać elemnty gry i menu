			al_draw_bitmap(lampa, 0, 0, NULL);
			al_draw_bitmap(lampa, 0, 0, NULL);
			al_draw_bitmap(artefakt3_1, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL); //1-22
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
			al_draw_bitmap(wrog3, 0, 0, NULL);
